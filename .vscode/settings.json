{
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.options": {
    "parserOptions": {
      "project": [
        "${workspaceFolder}/tsconfig.json"
      ],
    }
  },
  "path-intellisense.autoSlashAfterDirectory": false,
  "path-intellisense.mappings": {
      "~$images": "${workspaceRoot}/src/assets/images",
  }
}
