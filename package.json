    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.9.0",
    "axios": "^0.21.1",
    "connected-react-router": "^6.5.2",
    "express": "^4.17.1",
    "history": "^4.9.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-hot-loader": "^4.12.12",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.0.5",
    "reduxsauce": "^1.1.0"
  }
}
