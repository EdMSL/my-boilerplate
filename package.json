{
  "name": "react-typescript-boilerplate",
  "version": "1.0.0",
  "description": "React boilerplate with TypeScript for new projects",
  "main": "index.tsx",
  "author": "Eduard Trayt <eduard2011rus@gmail.com> (https://github.com/EdMSL)",
  "repository": {
    "type": "git",
    "url": "https://github.com/EdMSL/react-typescript-boilerplate.git"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "redux",
    "typescript"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "postcss": "postcss src/**/*.scss --config ./config/postcss.config.js --replace",
    "stylelint": "stylelint src/**/*.scss --config ./.stylelintrc-full.js --syntax scss --fix",
    "eslint": "eslint --config ./.eslintrc-full.js src --ext .ts,.tsx,.js,.jsx --fix",
    "tslint": "tsc --project tsconfig.json",
    "lint": "npm run stylelint && npm run eslint && npm run tslint",
    "test": "mocha --no-package --config ./.mocharc.js",
    "init": "node ./scripts/init.js && git remote rm origin",
    "start": "cross-env NODE_ENV=development webpack serve --mode development --config=./config/webpack.dev.config.js --progress",
    "start:prod": "node server.js",
    "build": "rm -rf ./build/* && cross-env NODE_ENV=production webpack --env production --config=./config/webpack.build.config.js --progress",
    "finish": "npm shrinkwrap"
  },
  "lint-staged": {
    "src/**/*.scss": [
      "npm run postcss",
      "npm run stylelint",
      "git add"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "npm run eslint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run tslint && lint-staged"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.6.0",
    "@types/chai": "4.2.3",
    "@types/classnames": "^2.2.9",
    "@types/mocha": "5.2.7",
    "@types/node": "^12.7.4",
    "@types/react": "^16.9.2",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "5.1.5",
    "@typescript-eslint/eslint-plugin": "^2.4.0",
    "@typescript-eslint/parser": "^2.4.0",
    "autoprefixer": "*9.6.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^3.2.0",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "chai": "4.2.0",
    "classnames": "^2.2.6",
    "copy-webpack-plugin": "~8.1.1",
    "cross-env": "^5.2.1",
    "css-loader": "^5.2.0",
    "css-mqpacker": "7.0.0",
    "eslint": "^6.3.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-mocha": "6.1.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "html-webpack-plugin": "5.3.1",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "mini-css-extract-plugin": "^1.4.0",
    "mocha": "^8.2.1",
    "node-sass": "^5.0.0",
    "normalize-scss": "*7.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "path": "^0.12.7",
    "postcss": "8.2.9",
    "postcss-cli": "^8.3.1",
    "postcss-color-converter": "^1.0.0",
    "postcss-loader": "^5.2.0",
    "postcss-safe-parser": "^5.0.2",
    "postcss-scss": "^3.0.5",
    "sass-loader": "^11.0.1",
    "sass-resources-loader": "^2.1.1",
    "source-map-loader": "^2.0.1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.8.0",
    "stylelint-at-rule-no-children": "^0.3.1",
    "stylelint-config-standard": "20.0.0",
    "stylelint-declaration-use-variable": "^1.7.0",
    "stylelint-order": "4.1.0",
    "stylelint-scss": "^3.10.0",
    "svg-spritemap-webpack-plugin": "^3.9.1",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "3.9.0",
    "typescript": "^3.6.3",
    "webpack": "5.30.0",
