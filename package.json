{
  "name": "my-react-typescript-boilerplate",
  "version": "1.0.0",
  "description": "My React TypeScript boilerplate for new projects",
  "main": "index.js",
  "author": "Eduard Trayt <eduard2011rus@gmail.com> (https://github.com/EdMSL)",
  "repository": {
    "type": "git",
    "url": "/"
  },
  "license": "ISC",
  "keywords": [],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie <= 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "postcss": "postcss src/**/*.scss --config ./config/postcss.config.js --replace",
    "stylelint": "prettier --write src/**/*.scss && stylelint src/**/*.scss --config ./.stylelintrc-full.js --fix",
    "eslint": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "lint": "npm run eslint && tsc --noEmit",
    "test": "npm run stylelint && npm run eslint",
    "start": "cross-env NODE_ENV=development webpack-dev-server --mode development --config=./config/webpack.dev.config.js --progress",
    "build": "cross-env NODE_ENV=production webpack --env production --config=./config/webpack.build.config.js --progress"
  },
  "lint-staged": {
    "src/**/*.scss": [
      "npm run stylelint",
      "git add"
    ],
    "src/**/*.{js,ts,jsx,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@babel/cli": "~7.5.5",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "~7.5.5",
    "@babel/preset-react": "~7.0.0",
    "@types/classnames": "^2.2.9",
    "@types/node": "^12.7.4",
    "@types/react": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "2.1.0",
    "@typescript-eslint/parser": "2.1.0",
    "autoprefixer": "*9.6.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "3.2.0",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "5.2.1",
    "css-loader": "^3.2.0",
    "css-mqpacker": "7.0.0",
    "eslint": "^6.3.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.2.0",
    "eslint-import-resolver-babel-module": "5.1.0",
    "eslint-import-resolver-webpack": "0.11.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "normalize-scss": "*7.0.1",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "path": "^0.12.7",
    "postcss-cli": "^6.1.3",
    "postcss-loader": "^3.0.0",
    "postcss-safe-parser": "^4.0.1",
    "postcss-scss": "^2.0.0",
    "prettier": "^1.18.2",
    "sass-loader": "^8.0.0",
    "sass-resources-loader": "^2.0.1",
    "source-map-loader": "^0.2.4",
    "style-loader": "1.0.0",
    "stylelint": "^10.1.0",
    "stylelint-at-rule-no-children": "^0.3.1",
    "stylelint-color-format": "^0.2.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-declaration-use-variable": "^1.7.0",
    "stylelint-order": "^3.1.0",
    "stylelint-scss": "^3.10.0",
    "terser-webpack-plugin": "^2.0.1",
    "ts-node": "~8.3.0",
    "typescript": "~3.6.2",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@hot-loader/react-dom": "~16.9.0",
    "connected-react-router": "~6.5.2",
    "history": "~4.9.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-hot-loader": "^4.12.12",
    "react-redux": "~7.1.1",
    "react-router-dom": "~5.0.1",
    "redux": "~4.0.4",
    "redux-persist": "~6.0.0",
    "redux-saga": "~1.0.5",
    "reduxsauce": "~1.1.0"
  }
}
